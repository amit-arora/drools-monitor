<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.lucazamador.drools.monitoring.model.kbase.KnowledgeBaseInfo" table="kbase_info">
    <id name="id" type="java.lang.Long">
      <column name="id" />
      <generator class="identity" />
    </id>
    <property name="knowledgeBaseId" type="string">
      <column name="knowledgeBaseId" length="100"/>
    </property>
    <property name="agentId" type="string">
      <column name="agentId" length="50" />
    </property>
  </class>
</hibernate-mapping>
